{
  "manifest_version": 2,

  "name": "Movie Info Finder",
  "description": "Find the info's of all movies.",

  "version": "0.2",
"browser_action": {
      "default_icon": {
          "19": "images/mov.png",
          "38": "images/mov.png"
      },
      "default_title": "Movie Info Finder",
      "default_popup": "Mif/index.html"
  },
  "permissions": [
      "tabs",
      "storage",
      "http://*/*",
      "https://*/*"
  ]
}